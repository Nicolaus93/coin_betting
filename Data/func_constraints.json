        "params": ["xs", "xe", "fs", "fprime"],
        "scaling":[0, 10],
        "limit":[["xs", 0, 1], ["xe", 0, 1]],
        "max_iterations": 1e4
    },
    "loss_quadratic":{
        "params": ["xs", "xe", "fs", "fprime"],
        "scaling":[0, 10],
