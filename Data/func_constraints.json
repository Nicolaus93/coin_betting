{   "AbsoluteLoss":{
        "params": ["xs", "xe", "offset", "slope"],
        "scaling":[0, 10],
        "limit":[["xs", 0, 1], ["xe", 0, 1]],
        "max_iterations": 1e4
    },
    "QuadraticLoss":{
        "params": ["xs", "xe", "offset", "slope"],
        "scaling":[0, 10],
        "limit":[["xs", 0, 1], ["xe", 0, 1]],
        "max_iterations":1e4
    },
    "GaussianLoss": {
        "params": ["mu", "sd"],
        "scaling": [0, 5],
        "limit":[["mu", "sd", 2], ["mu", "sd", 2]],
        "max_iterations":1e4

    },
    "SinusoidalLoss": {
        "scaling":[0, 10],
        "max_iterations": 1e4,
        "limit":[[0, 0, 1], [10, 0, 1]]
    },
    "SyntheticLoss": {
        "scaling":[0, 1],
        "max_iterations": 1e6,
        "limit":[[0, 0, 1], [1, 0, 1]]
    }
}